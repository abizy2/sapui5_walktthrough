<mvc:View 
  controllerName="namespace.controller.catalog"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="Каталог">
		<HBox
			id="catalogList"
			items="{main>/catalog}">
			<GenericTile
				header="{main>title}"
				subheader="{main>ExtendedPrice}"
				headerImage="{main>photo}">
					<TileContent footer="{main>Quantity}">
					<VBox>
						<Button text="Buy" press="onPress" > </Button> 
						</VBox>
					</TileContent>
			</GenericTile>
		</HBox>
	</Page>
</mvc:View>