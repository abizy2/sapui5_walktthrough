<mvc:View 
  controllerName="namespace.controller.catalog"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="Каталог">
		 <TileContainer
			id="catalogList"
			tiles="{main>/catalog}">
			<StandardTile
			title="{main>title}"
			number="{main>ExtendedPrice}"
			icon="{main>photo}"
			/>
			<!-- <TileContent>
		</TileContent> -->
			
		</TileContainer>
	</Page>
</mvc:View>