<mvc:View 
  controllerName="namespace.controller.busket"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="Корзина">
		<List
			id='BusketList'
			items="{main>/busket}">
			<StandardListItem
				title="{main>title}"
				description="{main>type}"
				type='Active'
				press="omCatalogItemPress"/>
		</List>
	</Page>
					
</mvc:View>