{
	"sap.app": {
		"id": "SplitApp",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "SplitApp.view.menu",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"config": {
			"sample": {
				"files": [
					"menu.view.xml",
					"C.controller.js",
					"manifest.json"
				]
			}
		},
				"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.walkthrough.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			}
		},
			"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
		  "config": {
			"routerClass": "sap.m.routing.Router",
			"viewType": "XML",
			"viewPath": "sap.m.sample.SplitApp.view.menu",
			"controlId": "SplitApp",
			"controlAggregation": "pages",
			"async": true
		  },
			"routes": [
				{
				  "pattern": "",
				  "name": "menu",
				  "target": "menu"
				},
				
				{
					"pattern": "page",
					"name": "page",
					"target":[
								"menu",
								"page"
							  ]
				},
				
							{
					  "pattern": "busket",
					  "name": "busket",
					  "target": [
						"menu",
						"busket"
					  ]
					},
					
							{
					  "pattern": "about",
					  "name": "about",
					  "target": [
						"menu",
						"about"
					  ]
					}
				
				],
				"targets": {
					"menu": {
					   "viewId": "menu",
					   "viewName": "menu"
					},
					"page": {
					  "viewId": "page",
					  "viewName": "Page"
						}
				 }
			}
		}
	}