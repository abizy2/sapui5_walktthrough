{
	"sap.app": {
		"id": "sap.m.sample.SplitApp",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.m.sample.SplitApp.view.menu",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"config": {
			"sample": {
				"files": [
					"menu.view.xml",
					"C.controller.js",
					"manifest.json"
				]
			}
		},
				"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.walkthrough.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			}
		},
			"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}