{
	"sap.app": {
		"id": "SplitApp",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "SplitApp.view.menu",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"config": {
			"sample": {
				"files": [
					"menu.view.xml",
					"C.controller.js",
					"manifest.json"
				]
			}
		},
				"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.walkthrough.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			}
		},
			"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
		  "config": {
			"routerClass": "sap.m.routing.Router",
			"viewType": "XML",
			"viewPath": "sap.m.sample.SplitApp.view.menu",
			"controlId": "SplitApp",
			"controlAggregation": "pages"
		  },
			"routes": [
				{
					"pattern": "detail/{component}",
					"name": "detail",
					"target": "detail"
				},
				{
					"pattern": "Links",
					"name": "LinksList",
					"target": ["LinksList","linkMP"]
				},
				{
					"pattern": "detail/{LinkToket}",
					"name": "LinkShow",
					"target": ["LinksList","LinkShow"]
				}
			],
		  "targets": {
				"LinksShow": {
					"viewName": "LinksList",
					"parent": "splitApp",
					"controlId": "splitApp",
					"controlAggregation": "menuController"
				},
				"detail": {
					"LinksList": {
						"viewName": "LinksList",
						"parent": "splitApp",
						"controlId": "splitApp",
						"controlAggregation": "pageController"
					}
				}
			}
		}
	}
}